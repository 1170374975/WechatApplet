.page-section{
  width: 90%;
  display: flex;
  flex-direction: column;
  background: #fff;
  border-radius: 10rpx;
  padding: 10rpx;
  margin: 4%;
  position: relative;
}
.item-name{
  font-size: 30rpx;
  margin: 10rpx 0 10rpx 30rpx;
}
.img{
  width: 200rpx;
  height: 200rpx;
  margin: 10rpx 0 10rpx 30rpx;
}
.fabu{
  position: fixed;
  bottom: 100rpx;
  right: 30rpx;
  width: 80rpx;
  height: 80rpx;
}
.delete{
  width: 60rpx;
  height: 60rpx;
  position: absolute;
  top: 60rpx;
  right: 30rpx;
}